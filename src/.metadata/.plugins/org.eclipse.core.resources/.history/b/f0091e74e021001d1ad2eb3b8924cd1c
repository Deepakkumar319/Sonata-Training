package com.sonata2;

public class Savingexamples {

	
		int accNo;
		String accName;
		double balance;
		
		Savingexamples(){}
		Savingexamples(int n,String name,double b){
			this.accNo=n;
			this.accName=name;
			this.balance=b;
		}
		public void deposite() {
			System.out.println(this.accNo);
		}
		public void withDraw() {}
		public static void main(String[] args) {
		Savingexamples s1=new Savingexamples();
		s1.accNo=123;
		s1.accName="Deepak";
		s1.balance=345;
		s1.deposite();

	}

}
